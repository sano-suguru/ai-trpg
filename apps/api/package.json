  "devDependencies": {
    "@ai-trpg/eslint-config": "workspace:*",
    "@ai-trpg/typescript-config": "workspace:*",
    "@cloudflare/workers-types": "^4.20260101.0",
    "@types/node": "^25.0.3",
    "drizzle-kit": "0.31.8",
